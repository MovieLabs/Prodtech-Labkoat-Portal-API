{
  "name": "service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@aserto/aserto-node": "^0.21.1",
    "@auth0/fga": "^0.9.1",
    "@aws-sdk/client-secrets-manager": "^3.293.0",
    "@okta/jwt-verifier": "^3.0.1",
    "@okta/okta-sdk-nodejs": "^6.6.0",
    "body-parser": "^1.20.1",
    "btoa": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.3.0",
    "express-jwt-aserto": "^1.0.75",
    "jsonata": "^2.0.1",
    "jwks-rsa": "^3.0.1",
    "node-fetch": "^2.6.9",
    "request-promise": "^4.2.6"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "extends": [
      "airbnb"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "func-names": [
        "error",
        "as-needed"
      ],
      "linebreak-style": 0,
      "indent": [
        "warn",
        4,
        {
          "VariableDeclarator": 1,
          "SwitchCase": 1
        }
      ],
      "react/jsx-indent": [
        "warn",
        4
      ],
      "react/jsx-indent-props": [
        "error",
        4
      ],
      "no-underscore-dangle": [
        0
      ],
      "no-console": "off",
      "max-len": [
        "warn",
        {
          "comments": 150,
          "code": 120
        }
      ]
    }
  },
  "devDependencies": {
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5"
  }
}
